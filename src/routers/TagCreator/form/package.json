{
  "name": "taxonomy-tag-creator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Standalone svelte app to create taxonomy dataset tag files",
  "scripts": {
    "dev": "vite",
    "build:theme": "smui-theme compile dist/smui.css -i src/theme",
    "build": "vite build && npm run build:theme",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@smui/button": "^7.0.0-beta.14",
    "@smui/circular-progress": "^7.0.0-beta.14",
    "@smui/icon-button": "^7.0.0-beta.14",
    "@smui/snackbar": "^7.0.0-beta.14",
    "@smui/top-app-bar": "^7.0.0-beta.14",
    "@sveltejs/vite-plugin-svelte": "^2.4.4",
    "@tsconfig/svelte": "^5.0.0",
    "sass": "^1.65.1",
    "smui-theme": "^7.0.0-beta.14",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-jsonschema-form": "github:webgme/svelte-jsonschema-form#v0.8.2",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
